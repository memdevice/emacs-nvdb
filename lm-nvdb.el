  "Words listed by alphabetic order: the first 7000+ most basic words from the NVdB by T. De Mauro (XXXXXXXXX  words).
Taken from this website:
https://www.dropbox.com/s/mkcyo53m15ktbnp/nuovovocabolariodibase.pdf")

;; Quindi FO = 2001; FO+AU = 4232; FO+AU+AD = 7243 = NVdB!

(defvar 2001-words-regexp
  (regexp-opt
   (cl-loop for i from 0 to 2000
	 collect (nth i 7243-most-frequent-words))
   'words)
  "Regular expression matching the first 2001 most frequent words
based on `7243-most-frequent-words'.")

(defvar 4232-words-regexp
  (regexp-opt
   (cl-loop for i from 2001 to 4231
	 collect (nth i 7243-most-frequent-words))
   'words)
  "Regular expression matching the first 4232 most frequent words
based on `7243-most-frequent-words'.")

(defvar 7243-words-regexp-a
  (regexp-opt
   (cl-loop for i from 4232 to 5999
	 collect (nth i 7243-most-frequent-words))
   'words)
  "Regular expression matching the first 7243 most frequent words
based on `7243-most-frequent-words'. (first)")

(defvar 7243-words-regexp-b
  (regexp-opt
   (cl-loop for i from 6000 to 7242
	 collect (nth i 7243-most-frequent-words))
   'words)
  "Regular expression matching the first 7243 most frequent words
based on `7243-most-frequent-words'. (second)")


;; LM: nota arrivando a 4999 smette di funzionare, a 4499 invece funziona
;; LM: ho risolto spezzando in tre liste da meno di 2400 parole l'una,

(define-derived-mode 7243-words-mode text-mode "FO-AU-AD"
  "Major mode for writing text limited to the most common words.
The words used are in `7243-most-frequent-words'."
  (font-lock-add-keywords nil `((,2001-words-regexp . 'scroll-bar)
   				(,4232-words-regexp . 'scroll-bar)
                (,7243-words-regexp-a . 'scroll-bar)
                (,7243-words-regexp-b . 'scroll-bar)
				("\\w+" . 'bold)))
  (setq font-lock-keywords-case-fold-search t))

(define-derived-mode 4232-words-mode text-mode "FO-AU"
  "Major mode for writing text limited to the most common words.
The words used are in `7243-most-frequent-words'."
  (font-lock-add-keywords nil `((,2001-words-regexp . 'scroll-bar)
   				(,4232-words-regexp . 'scroll-bar)
				("\\w+" . 'deafult)))
  (setq font-lock-keywords-case-fold-search t))

(define-derived-mode 2001-words-mode text-mode "FO"
  "Major mode for writing text limited to the most common words.
The words used are in `7243-most-frequent-words'."
  (font-lock-add-keywords nil `((,2001-words-regexp . 'scroll-bar)
				("\\w+" . 'default)))
  (setq font-lock-keywords-case-fold-search t))


